<Context>
	<Parameter name="dependencyProviderClassName"
		value="se.uu.ub.cora.therest.initialize.TheRestDependencyProvider"
		override="false" />
	<Parameter name="gatekeeperURL"
		value="http://gatekeeper:8080/gatekeeperserver/" override="false" />
	<Parameter name="storageOnDiskBasePath"
		value="/mnt/data/basicstorage/" override="false" />
	<Parameter name="solrURL"
		value="http://solr:8983/solr/coracore" override="false" />
	<Parameter name="theRestPublicPathToSystem"
		value="/alvin/rest/" override="false" />

	<Parameter name="storageType" value="memory" override="false" />


	<Parameter name="fedoraArchiveURL"
		value="http://alvin-docker-fedora:8080/fcrepo/rest/" override="false" />
	<Parameter name="fedoraUsername" value="fedoraAdmin"
		override="false" />
	<Parameter name="fedoraPassword" value="changeit"
		override="false" />

	<Parameter name="messageServerHostname"
		value="messaging.alvin-portal.org" override="false" />
	<Parameter name="messageServerPort" value="5672"
		override="false" />

	<Parameter name="coraDatabaseLookupName"
		value="java:/comp/env/jdbc/postgres" override="false" />
	<Resource name="jdbc/postgres" auth="Container"
		type="javax.sql.DataSource" driverClassName="org.postgresql.Driver"
		url="jdbc:postgresql://alvin-docker-postgresql:5432/alvin"
		username="alvin" password="alvin" maxTotal="20000"
		maxIdle="10" maxWaitMillis="-1" />
</Context>